{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "max-len": ["warn", 120],
        "no-trailing-spaces": ["warn", { "skipBlankLines": true }],
        "eol-last": ["warn", "always"]
    }
  }
